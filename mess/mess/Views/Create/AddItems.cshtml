@{
    Layout = null;
}
<html>
<head>
 <link rel="icon" href="~/MyTemp/img/favicon.png" type="image/png" sizes="16xfa16">
<meta charset="utf-8"/>
<meta name="veiwport" content="width=device-width, initial-scale=1"/>
<link href="~/MyTemp/css/bootstrap.css" rel="stylesheet" type="text/css" />
<script src="~/MyTemp/js/bootstrap.js" type="text/javascript"></script>
<title>eReserve</title>
</head>

<body>
<div id="custom-bootstrap-menu" class="navbar navbar-default " role="navigation">
 <div class="navbar-header"><a class="navbar-brand" href="#">DYNAMICS</a>
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-menubuilder"><span class="sr-only">Toggle navigation</span><span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>
            </button>
            </div>
             <div class = "collapse navbar-collapse navbar-menubuilder">
        <div class="navbar navbar-custom">
    <ul class="nav navbar-nav">
    <li>@Html.ActionLink("Users", "Display", "Create")</li>
    <li>@Html.ActionLink("Admins", "Admins", "Create")</li>
    </ul>
    <ul class = "nav navbar-nav navbar-right">
     <li>@Html.ActionLink("Logout", "Logout", "Create")</li>
</ul>
        </div>
         </div>
        </div>
 
 @model mess.EntFrame.Item_Table

@using (Html.BeginForm())
{             

    <div class="Container">
        <div class=" registrationform">
        <tr><td><center><img src="~/MyTemp/img/reg.png" height="100px" width="100px"/></center></td></tr> 
        <center><h2><span style="color: white">eReserve Registration</span></h2></center>
         <div class="container" style="width:40%; margin-top:2%">
            
            <center><h5>@Html.TextBoxFor(e => e.ItemID, new { @class = "form-control", @placeholder = "Item ID" }) <span style="color:Red"> 
             @Html.ValidationMessageFor(v => v.ItemID, "", new { @class = "error" })</span></h5></center>

            <center><h5>@Html.TextBoxFor(e => e.item, new { @class = "form-control", @placeholder = "Item Name" }) <span style="color:Red">
             @Html.ValidationMessageFor(v => v.item, "", new { @class = "error" })</span></h5></center>

            <center><h5>@Html.TextBoxFor(e => e.quant, new { @class = "form-control", @placeholder = "Quantity" }) <span style="color:Red"> 
            @Html.ValidationMessageFor(v => v.quant, "", new { @class = "error" })</span></h5></center>

             <input type="submit" value="ADD" class="btn btn-block btn-primary" />
        </div>
        </div>
    </div>
}
</body>
</html>  
